{
  "name": "Facebook Chat Resizer",
  "description": "Resize the Facebook chat window",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*", "*://facebook.com/*"],
      "js": ["javascript/jquery-2.1.3.min.js", "javascript/extension.js"],
      "css": ["css/style.css"]
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
       "images/resize.png"
     ]
}
