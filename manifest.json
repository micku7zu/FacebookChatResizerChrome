{
  "name": "Facebook Chat Resizer",
  "description": "Resize the facebook chat window",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*", "*://facebook.com/*"],
      "js": ["jquery-2.1.3.min.js", "myscript.js"],
      "css": ["style.css"]
    }
  ],
  "manifest_version": 2,
  "web_accessible_resources": [
       "resize.png"
     ]
}
